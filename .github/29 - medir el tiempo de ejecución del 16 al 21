import time
import random
import statistics

num_rand = []
med_num_rand = []
mediana_num_rand =[]
var_num_rand=[]
minimo_num_rand = []
maximo_num_rand = []

def genrnd(a, b):
    for i in range(50):
        num_rand.append(random.uniform(a,b))
      
def media(c):
    med_num_rand = statistics.mean(c)
    return med_num_rand

def mediana(d):
    mediana_num_rand = statistics.median(d)
    return mediana_num_rand

def rango(e):
    rango_num_rand = len(e)
    return rango_num_rand

def varianza(f):
    var_num_rand = statistics.variance(f)
    return var_num_rand
        
def minimo(g):
    minimo_num_rand = min(g)
    return minimo_num_rand

def maximo(h):
    maximo_num_rand = max(h)
    return maximo_num_rand

start = time.perf_counter()

genrnd(100,200)


end = time.perf_counter()
print(f'Tiempo de ejecuciÃ³n: {end - start} segundos.')
